<div class="container mt-5" style="max-width: 90%">

  <div class="row row-cols-3">

    <div class="col-lg-3">

      <div class="user-container">

      </div>

    </div>

    <div class="col-lg-6">

      <div class="post-container mb-5">

        <%= render 'shared/posts-feed' %>

      </div>

      <div class="title-feed-container mb-2">
        <div class="container d-flex justify-content-around align-items-center">
          <h1>main feed</h1>
          <span class="dot"></span>
          <h1>profile feed</h1>
        </div>
      </div>

      <div class="feed-container">

      </div>

    </div>

    <div class="col-lg-3">

      <div class="search-container mb-5">

      </div>

      <div class="title-msgs-container mb-2">
        <div class="container d-flex justify-content-around align-items-center">
          <h1>messages</h1>
          <span class="dot"></span>
          <h1>notifications</h1>
        </div>
      </div>

      <div class="msg-container">

      </div>

    </div>

  </div>

</div>
