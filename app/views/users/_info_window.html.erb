<%= cl_image_tag display_photo(user), aspect_ratio: 1, crop: :fill, class: "avatar" %>
<h3 class="mt-3"><%= user.first_name %> <%= user.last_name %></h3>
<%= link_to user_path(user) do %>
  <p class="mb-3">@<%= user.username %></p>
<% end %>
<p>Plays:</p>
<% user.skills.each do |skill| %>
  <p><%= skill.instrument %> | <%= skill.experience %></p>
<% end %>
